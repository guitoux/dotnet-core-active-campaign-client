<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netcoreapp2.1</TargetFramework>
        <Authors>FabienP</Authors>
        <PackageId>unitee.activecampaignapiclient</PackageId>
        <Version>1.0.3</Version>
        <Title>.NET Core Active Campaign client</Title>
        <Description>A .NET Core api client for Active Campaign V2 API</Description>
        <PackageProjectUrl>https://www.nuget.org/packages/ActiveCampaignApiClient/</PackageProjectUrl>
        <RepositoryUrl>https://github.com/uniteeio/dotnet-core-active-campaign-client</RepositoryUrl>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="2.1.1"/>
        <PackageReference Include="Microsoft.Extensions.Http" Version="2.1.1"/>
        <PackageReference Include="Microsoft.Extensions.Options.ConfigurationExtensions" Version="2.1.1"/>
        <PackageReference Include="Newtonsoft.Json" Version="11.0.2"/>
    </ItemGroup>

</Project>
